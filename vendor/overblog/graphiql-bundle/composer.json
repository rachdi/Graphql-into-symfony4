{
    "name": "overblog/graphiql-bundle",
    "type": "symfony-bundle",
    "description": "Symfony GraphiQLBundle makes possible to render the UI into your symfony project",
    "keywords": [
        "GraphQL", "GraphiQL"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Renato Mefi",
            "email": "renato@mefi.in"
        },
        {
            "name": "Overblog",
            "homepage": "http://www.over-blog.com"
        }
    ],
    "config" : {
        "sort-packages": true
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/http-foundation": "^3.2.2 || ^4.0",
        "symfony/twig-bundle": "^3.2.10 || ^4.0",
        "symfony/routing": "^3.2 || ^4.0"
    },
    "require-dev": {
        "overblog/graphql-bundle": "^0.9.0 || ^0.10.0",
        "phpunit/phpunit": "^5.7 || ^6.0",
        "sensio/framework-extra-bundle": "^3.0 || ^4.0",
        "symfony/browser-kit": "^3.2 || ^4.0",
        "symfony/config": "^3.2 || ^4.0",
        "symfony/console": "^3.2 || ^4.0",
        "symfony/dependency-injection": "^3.2 || ^4.0",
        "symfony/expression-language": "^3.2 || ^4.0",
        "symfony/framework-bundle": "^3.2.2 || ^4.0",
        "symfony/templating": "^3.2 || ^4.0",
        "symfony/yaml": "^3.2 || ^4.0"
    },
    "autoload": {
        "psr-4": { "Overblog\\GraphiQLBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    }
}
